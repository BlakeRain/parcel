  "devDependencies": {
    "@types/preact-custom-element": "^4.0.4",
    "autoprefixer": "^10.4.14",
    "cypress": "^13.13.2",
    "esbuild": "^0.23.0",
    "htm": "^3.1.1",
    "htmx.org": "^2.0.1",
    "htmx-ext-loading-states": "^2.0.0",
    "lucide-static": "^0.424.0",
    "postcss-import": "^16.1.0",
    "preact": "^10.23.1",
    "preact-custom-element": "^4.3.0",
    "sqlite3": "^5.1.7",
    "start-server-and-test": "^2.0.5",
