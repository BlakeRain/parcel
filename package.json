{
  "name": "parcel",
  "version": "1.0.1",
  "author": "Blake Rain <blake.rain@blakerain.com>",
  "license": "BSD3",
  "devDependencies": {
    "autoprefixer": "^10.4.14",
    "htmx.org": "^1.9.6",
    "hyperscript.org": "^0.9.11",
    "tailwindcss": "^3.3.3"
  },
  "scripts": {
    "build-dev": "tailwindcss --postcss -i style/main.css -o static/main.css",
    "build": "tailwindcss --postcss -i style/main.css -o static/main.css -m",
    "copy-dev": "cp node_modules/htmx.org/dist/htmx.js static/htmx.js && cp node_modules/hyperscript.org/dist/_hyperscript.js static/hyperscript.js",
    "copy": "cp node_modules/htmx.org/dist/htmx.min.js static/htmx.js && cp node_modules/hyperscript.org/dist/_hyperscript.min.js static/hyperscript.js"
  }
}
